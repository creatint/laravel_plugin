{
    "name": "gallery/plugin",
    "description": "Laravel通用插件系统。An universal plugin system for laravel.",
    "keywords": [
        "Laravel",
        "PHP",
        "Plugin System",
        "Plugin Market",
        "Hook",
        "插件",
        "插件市场",
        "钩子"
    ],
    "homepage": "https://github.com/tianshuapp/gallery_plugin",
    "authors": [
        {
            "name": "Wangds",
            "email": "creatint@qq.com"
        }
    ],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.0.0"
    },
    "autoload": {
        "psr-4": {
            "Gallery\\Plugin\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gallery\\Plugin\\PluginServiceProvider"
            ]
        }
    },
    "require-dev": {
        "laravel/framework": "^5.8"
    }
}
